{
  "name": "px2em-converter",
  "version": "1.1.0",
  "private": true,
  "dependencies": {
    "core-js": "^3.4.1",
    "intl": "^1.2.5",
    "raf": "^3.4.1",
    "rc-notification": "^4.4.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-intl": "^4.6.9",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.2.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.1",
    "@storybook/react": "^5.3.19",
    "@testing-library/react-hooks": "^3.3.0",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "esdoc-unexported-identifier-plugin": "^1.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^3.1.0",
    "jest-localstorage-mock": "^2.4.2",
    "jest-styled-components": "^7.0.2",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-app-rewired": "^2.1.5",
    "react-test-renderer": "^16.13.1",
    "replace": "^1.1.5",
    "standard-version": "^8.0.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "GENERATE_SOURCEMAP=false react-app-rewired build",
    "test": "react-app-rewired test --verbose --coverage --collectCoverageFrom='[\"src/utils/**/*.{js,jsx}\", \"src/components/**/*.{js,jsx}\", \"src/hooks/**/*.{js,jsx}\"]'",
    "test:ci": "react-app-rewired test --coverage --collectCoverageFrom='[\"src/utils/**/*.{js,jsx}\", \"src/components/**/*.{js,jsx}\", \"src/hooks/**/*.{js,jsx}\"]' --watchAll=false --ci --runInBand",
    "eject": "react-scripts eject",
    "first-release": "standard-version --commit-all --tag-prefix v --first-release",
    "release": "standard-version --commit-all --tag-prefix v",
    "lint": "eslint --ext .js,.jsx src",
    "lint:css": "stylelint './src/**/*.js'",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public --output-dir docs/storybook",
    "esdoc": "./node_modules/.bin/esdoc"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "jest-styled-components"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@components(.*)$": "<rootDir>/src/components/$1",
      "^@containers(.*)$": "<rootDir>/src/containers/$1",
      "^@fonts(.*)$": "<rootDir>/src/assets/fonts/$1",
      "^@hooks(.*)$": "<rootDir>/src/hooks/$1",
      "^@images(.*)$": "<rootDir>/src/assets/images/$1",
      "^@locale(.*)$": "<rootDir>/src/locale/$1",
      "^@pages(.*)$": "<rootDir>/src/pages/$1",
      "^@providers(.*)$": "<rootDir>/src/providers/$1",
      "^@routes(.*)$": "<rootDir>/src/routes/$1",
      "^@styles(.*)$": "<rootDir>/src/styles/$1",
      "^@utils(.*)$": "<rootDir>/src/utils/$1"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 9",
      "not op_mini all",
      "ie 10",
      "ie 11"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not ie <= 9",
      "not op_mini all",
      "ie 10",
      "ie 11"
    ]
  }
}
